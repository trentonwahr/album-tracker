  <% if (!user) { %>
    <script defer src="https://accounts.google.com/gsi/client"></script>
  <% } %>
</head>
<body>
  <nav>
    <a 
      href="/" class=<%= title === 'Feed' ? 'active' : '' %>
    >
      FEED
    </a>
    <a 
      href="/albums/new" class=<%= title === 'Add Album' ? 'active' : '' %>
    >
      ADD ALBUM
    </a>
    <a 
      href="/albums" class=<%= title === 'All Albums' ? 'active' : '' %>
    >
      ALL ALBUMS
    </a>
    <a 
      href="/profiles/<%= profile._id %>" class=<%= title === 'Profile' ? 'active' : '' %>
    >
      PROFILE
    </a>
    <% if (user) { %>
      <a href="/auth/logout" class="right">
        <button type="submit">Logout</button>
      </a>
    <% } else { %>
      <div 
        id="g_id_onload"
        data-client_id="<%= googleClientID %>"
        data-login_uri="/auth/google"
        data-auto_prompt="false"
        data-prompt_parent_id="g_id_onload"
      ></div>
      <div 
        class="g_id_signin right"
        data-type="standard"
        data-size="large"
        data-theme="filled_black"
        data-text="sign_in_with"
        data-shape="rectangular"
        data-logo_alignment="left"
      ></div>
    <% } %>
  </nav>
